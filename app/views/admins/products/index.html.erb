  
<h1>Admins::Products#index</h1>
<p>Find me in app/views/admins/products/index.html.erb</p>
<div class="container">
  <div class="row">
      

    <div class="col-md-8 offset-md-1">
<h2>商品一覧</h2>
 <%= link_to "新規登録",admins_products_new_path %>
<table class="table table-inverse">
      <thead>
    <tr>
      <th></th>
      <th>商品名</th>
      <th>詳細</th>
      <th colspan="3"></th>
    </tr>
  </thead>
      <% @products.each do |product| %>
        <tr>
          <td>
    
            <%= attachment_image_tag product, :image, :fill, 60, 60, fallback: "no_image.jpg",size:"40x40"  %>
       
          </td>
          <td>
      
            <p><%= product.name%></p>
     
          </td>
          <td>
          <p><%= product.about%></p>
          </td>
        </tr>
      <% end %>
      </table>
     </div>


  
  </div>
</div>  